{
    "environments": {
        "prod": {
            "api_url": "{{ prod_api_url }}",
            "input_dir": "{{ prod_input_dir }}",
            "newspaper_output_dir": "{{ prod_newspaper_dir }}"
        },
        "dev": {
            "api_url": "{{ dev_api_url }}",
            "input_dir": "{{ dev_input_dir }}",
            "newspaper_output_dir": "{{ dev_newspaper_dir }}"
        }
    },
    "smb_shares": {
        "data": {
            "enabled": {{ smb_data_enabled | to_json }},
            "url": "{{ smb_data_url }}",
            "mount_point": "{{ smb_data_mount }}"
        },
        "input": {
            "enabled": {{ smb_input_enabled | to_json }},
            "url": "{{ smb_input_url }}",
            "mount_point": "{{ smb_input_mount }}"
        }
    },
    "poll_interval": {{ poll_interval }},
    "poll_timeout": {{ poll_timeout }},
    "auto_eject": {{ auto_eject | to_json }}
}
